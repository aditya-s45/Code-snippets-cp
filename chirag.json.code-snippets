			"",
			"//Number theory",
			"int gcd(int a, int b) { return b ? gcd(b, a % b) : a; }",
			"int lcm(int a, int b) { return a / gcd(a, b) * b; }",
			"int factorial(int n) { return (n==1 || n==0) ? 1: n * factorial(n - 1); } ",
			"bool isSquare(int x) { int s = sqrt(x); return (s * s == x); }",
			"bool isPowerOfTwo(int x) { return x && (!(x & (x - 1))); }",
			"bool prime(int n) { if (n <= 1) return false; for (int i = 2; i * i <= n; i++) if (n % i == 0) return false; return true; }",
			"int spf(int n){ for(int i=2;i*i<=n;i++){ if(n%i==0) return i; } return n; } ",
			"int countSetBits(int n){ int count = 0; while (n) { n &= (n - 1); count++; } return count; }",
			"int ncr(int n, int r) { int res = 1; if (r > n - r) r = n - r; for (int i = 0; i < r; i++) { res *= (n - i); res /= (i + 1); } return res; }",
			"int power(int x, int y) { int res = 1; while (y > 0) { if (y & 1) res = res * x; y = y >> 1; x = x * x; } return res; }",
			"int modpow(int x, int y, int p) { int res = 1; x = x % p; while (y > 0) { if (y & 1) res = (res * x) % p; y = y >> 1; x = (x * x) % p; } return res; }",
			"int modinv(int x, int p) { return modpow(x, p - 2, p); }",
